cat /etc/nginx/sites-enabled/novnc.your.tld

server {
  listen  10.10.202.200:80;
  server_name novnc.your.tld;
  return 301 https://$host$request_uri;
  access_log  /var/log/nginx/novnc.access.log;
  error_log /var/log/nginx/novnc.error.log;
}

server {
  listen 10.10.202.200:443;
  server_name novnc.your.tld;
  ssl on;
  ssl_certificate /etc/letsencrypt/live/novnc.your.tld/fullchain.pem;
  ssl_certificate_key /etc/letsencrypt/live/novnc.your.tld/privkey.pem;
  ssl_dhparam /etc/ssl/certs/dhparam.pem;
  access_log  /var/log/nginx/novnc.access.log;
  error_log /var/log/nginx/novnc.error.log; 
  root    /var/www/novnc.your.tld;

 location /websockify {
        proxy_read_timeout 61s;
        proxy_buffering off;
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "upgrade";
        proxy_pass http://127.0.0.1:6080$is_args$args;
        }

  location ~* /pass$ {
        try_files $uri /vnc/vnc_lite.html${is_args}${args} ;

        }
}
